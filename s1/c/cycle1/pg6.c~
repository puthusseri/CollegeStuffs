#include<stdio.h>
void main()
{
	char a;
	printf("ENTER A CHARACTER:");
	scanf("%c",&a);
	if(a>='a'&&a<='z')
	{
		
		printf("\nYOU ENTERED A LOWERCASE LETTER ");
		printf("\nTHe correponding upeercase is %c",a-32);
	}
	else
	{
		
		printf("\nYOU ENTERED A UPPERCASE LETTER ");
		printf("\nTHe correponding lowercase is %c",a+32);
	}
}
